{
  "name": "rkllmjs",
  "version": "0.1.0",
  "description": "High-performance JavaScript bindings for Rockchip LLM Runtime - run local LLMs on Rockchip NPUs",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "libs",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:node": "node --experimental-loader tsx ./node_modules/.bin/jest",
    "test:deno": "deno test --allow-ffi --allow-read",
    "clean": "rm -rf dist",
    "example": "bun run examples/bun-ffi-example.ts",
    "check-runtime": "bun run -e \"console.log(JSON.stringify(require('./src/runtime/detector.js').detectRuntime(), null, 2))\""
  },
  "keywords": [
    "rockchip",
    "npu",
    "llm",
    "ai",
    "inference",
    "rk3588",
    "rk356x",
    "edge-computing",
    "local-llm",
    "javascript",
    "typescript",
    "bun",
    "node",
    "deno",
    "ffi",
    "universal",
    "multi-runtime",
    "esm"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akaoio/rkllmjs.git"
  },
  "bugs": {
    "url": "https://github.com/akaoio/rkllmjs/issues"
  },
  "homepage": "https://github.com/akaoio/rkllmjs#readme",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=16.0.0",
    "deno": ">=1.25.0"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "arm64",
    "arm"
  ],
  "optionalDependencies": {
    "koffi": "^2.4.0",
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  }
}
