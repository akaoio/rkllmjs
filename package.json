{
  "name": "rkllmjs",
  "version": "0.1.0",
  "type": "module",
  "description": "Node.js native module for Rockchip RK3588 NPU - enabling JavaScript/TypeScript to leverage NPU through Rockchip's native library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc && npm run build:native",
    "build:native": "./scripts/build.sh",
    "build:ts": "tsc",
    "test": "npm run setup-test && npm run validate && npm run test:node",
    "setup-test": "./scripts/setup-test-env.sh",
    "test:unit": "npm run test:node",
    "test:node": "node --test --import tsx/esm src/**/*.test.ts",
    "test:bun": "bun test",
    "test:deno": "deno test --allow-all",
    "validate": "bash scripts/validate.sh",
    "cli": "node dist/cli-runner/cli-runner.js",
    "cli:bun": "bun src/cli-runner/cli-runner.ts",
    "cli:deno": "deno run --allow-all src/cli-runner/cli-runner.ts",
    "dev": "tsc --watch",
    "clean": "rm -rf dist build node_modules/.cache logs",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src --out docs",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "rockchip",
    "rk3588",
    "npu",
    "llm",
    "ai",
    "machine-learning",
    "typescript",
    "native-addon",
    "n-api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/rkllmjs.git"
  },
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "files": [
    "dist",
    "libs",
    "scripts",
    "README.md",
    "RULES.md"
  ],
  "devDependencies": {
    "@types/node": "^20.19.2",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.0.0",
    "glob": "^11.0.3",
    "node-addon-api": "^8.4.0",
    "node-gyp": "^11.2.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-fetch": "^3.3.2"
  },
  "optionalDependencies": {
    "bun-types": "^1.0.0"
  },
  "runtime": {
    "primary": "node",
    "supported": [
      "node",
      "bun",
      "deno"
    ],
    "experimental": [
      "bun",
      "deno"
    ]
  },
  "rkllm": {
    "version": "1.1.4",
    "architecture": "rk3588",
    "npu": {
      "supported": true,
      "library": "libs/rkllm/aarch64/librkllmrt.so",
      "headers": "libs/rkllm/include/rkllm.h"
    }
  }
}
